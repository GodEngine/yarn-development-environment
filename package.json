{
  "name": "hd",
  "version": "0.10.2",
  "description": "运营活动",
  "bin": {
    "webpack": "./node_modules/.bin/webpack",
    "karma": "./node_modules/karma/bin/karma",
    "http-server": "./node_modules/.bin/http-server"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack --hide-modules",
    "build": "NODE_ENV=production webpack --config webpack.config.babel.js --progress --hide-modules",
    "lint": "eslint ./src ./webpack.config.js -f table || true",
    "clean:coverage": "rimraf ./coverage",
    "test": "yarn run clean:coverage && karma start --single-run --browsers PhantomJS",
    "test:watch": "yarn run clean:coverage && karma start  --auto-watch --browsers PhantomJS",
    "coverage": "http-server -p 1234 ./coverage",
    "sprite": "webpack --config ./components/webpack.config.babel.js --progress --hide-modules"
  },
  "author": "LQ",
  "license": "MIT",
